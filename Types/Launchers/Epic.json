{
    "Key": "Epic",
    "Name": "Epic Games Launcher",
    "Defaults": {
        "LauncherClass": "SimpleLauncher",
        "LauncherExe": "EpicGamesLauncher.exe",
        "LauncherLocateMethod": "Registry",
        "LauncherLocateRegKey": "HKEY_CLASSES_ROOT\\com.epicgames.launcher\\shell\\open\\command",
        "LauncherLocateRegRemovePrefix": "\"",
        "LauncherLocateRegStripQuotes": true,
        "LauncherLocateRegRemoveSuffix": "\\EpicGamesLauncher.exe %1",
        "LauncherCloseMethod": "Auto",
        "LauncherCloseBeforeRun": true,
        "LauncherCloseAfterRun": true,
        "GameType": "Default",
        "GameRunMethod": "Run"
    }
}
